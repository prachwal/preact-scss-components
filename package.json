{
  "name": "preact-scss-components",
  "version": "0.0.1",
  "description": "Components library for preact with scss support",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "style": "dist/index.css",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "merge-src": "node scripts/merge-src.js",
    "prepublishOnly": "./scripts/check-version.sh"
  },
  "keywords": [
    "preact",
    "scss",
    "components",
    "library"
  ],
  "author": "Przemyslaw Rachwal",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prachwal/preact-scss-components.git"
  },
  "homepage": "https://github.com/prachwal/preact-scss-components#readme",
  "bugs": {
    "url": "https://github.com/prachwal/preact-scss-components/issues"
  },
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./style": "./dist/index.css"
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "preact": "^10.0.0"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.10.2",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/preact": "^3.2.4",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^24.10.0",
    "@vitest/coverage-v8": "^4.0.8",
    "@vitest/ui": "^4.0.8",
    "jsdom": "^26.0.0",
    "sass": "^1.93.3",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.8"
  },
  "dependencies": {
    "@preact/signals": "^2.4.0",
    "preact": "^10.0.0"
  }
}
